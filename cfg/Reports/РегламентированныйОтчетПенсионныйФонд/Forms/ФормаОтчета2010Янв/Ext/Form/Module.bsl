Функция ПолучитьИмяФайлаPFZ()

	Результат = "";
	
	// код органа ПФУ
	КодОргана = мПереченьТаблиц.Области.КодОрганаПФУ.Значение;
	КодОргана = ЛЕВ(СокрЛП(КодОргана) + "     ", 5);
    Результат = Результат + КодОргана;
	
	// код едрпоу, дополненный справа "Z"
	КодЕДРПОУ = мПереченьТаблиц.Области.ЕДРПОУ.Значение;
	КодЕДРПОУ = ЛЕВ(СокрЛП(КодЕДРПОУ) + "ZZZZZZZZZZ", 10);
    Результат = Результат + КодЕДРПОУ;
	
	// код филии, дополненный справа "Z"
	Филиал = мПереченьТаблиц.Области.Филиал.Значение;
	Филиал = ЛЕВ(СокрЛП(Филиал) + "ZZZZZ", 5);
    Результат = Результат + Филиал;
	
	//Дата формирования
	Дата = Формат(ДатаПодписи, "ДФ=ddMMyyyy");
	Результат = Результат + Дата;
	
	//Дата формирования
	Время = Формат(ТекущаяДата() , "ДФ=ЧЧммсс");
	Результат = Результат + Время;
	
	//ТипДокумента
	Результат = Результат + "P04";
	
	//версия док
	Результат = Результат + ВерсияВыгрузкиPFZ;
	
	// код центра сертификации ключей
	КодЦентраКлючей = мСлужебныеДанные.Области.КодЦентраКлючей.Значение;
	Если НЕ ЗначениеЗаполнено(КодЦентраКлючей) Тогда
		КодЦентраКлючей = "0";
	КонецЕсли;
	Результат = Результат + КодЦентраКлючей; 
	
	// расширение
	Результат = Результат + ".pfz";
	
    Возврат Результат; 
КонецФункции // ()

