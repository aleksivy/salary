Процедура ЗаписатьЭлементXML(XML, ИмяЭлементы, Значение, Тип = "строка", НеВыгружатьПустое = Ложь);
	
	Если НеВыгружатьПустое И Не ЗначениеЗаполнено(Значение) Тогда
		Результат = "";
	ИначеЕсли ВРЕГ(Тип) = "ЧИСЛОЦЕЛ" Тогда	
	    Результат = Формат(Значение,"ЧДЦ=0; ЧН=; ЧГ=");
	ИначеЕсли ВРЕГ(Тип) = "ЧИСЛОДЕС2" Тогда	
	    Результат = Формат(Значение,"ЧДЦ=2; ЧРД=.; ЧН=; ЧГ=");
	ИначеЕсли ВРЕГ(Тип) = "ЧИСЛОДЕС" Тогда	
	    Результат = Формат(Значение,"ЧН=; ЧРД=.; ЧГ=");
	ИначеЕсли ВРЕГ(Тип) = "ДАТА" Тогда	
	    Результат = Формат(Значение,"ЧГ=; ДФ=ddMMyyyy");
	Иначе// "строка"
		Результат = СокрЛП(Строка(Значение));
	КонецЕсли;
	
	XML.ЗаписатьНачалоЭлемента(ИмяЭлементы);
	XML.ЗаписатьТекст(XMLСтрока(Результат));	
	XML.ЗаписатьКонецЭлемента();	
	
КонецПроцедуры

