		<Алгоритм Имя="ПолучитьЛьготу" ИспользуетсяПриЗагрузке="false">
			<Текст>	Сотрудник				= ИсходящиеДанные;
	Дат						= ВходящиеДанные;
	Облагаемое				= Источник;
	ПорогПолученияСоцЛьгот	= Приемник;
	
	ВидЛьготы = "";  
	
	// определяем льготы для налога на доходы физ.лица  
	НачПериода = НачМесяца(Дат);
	КонПериода = КонМесяца(Дат); 
	//если льгота не действует надо переписпть из допПараметров уже вычесленную сумму уменьшения базы (основные сборы)
	Если (Облагаемое &gt; ПорогПолученияСоцЛьгот) 
	ИЛИ ( НачМесяца(КонПериода) = НачМесяца(Сотрудник.ДатаОкончанияДействия) ) // льгота НЕ применяется к месяцу увольнения 
	Тогда
		//нет права на социальные льготы
		Возврат ВидЛьготы;
	КонецЕсли;
	
	// получим значение необлагаемого минимума
	МинЗарплата = 0; спрМЗП = 0;
	Если глПолучитьСтавку(спрМЗП,"МинЗарплата","о минимальной зарплате") = 1 Тогда
		МинЗарплата = спрМЗП.Предел.Получить(НачГода(КонПериода));
	КонецЕсли;
	
	тбВсеЛьготы = СоздатьОбъект("ТаблицаЗначений"); 
	тбВсеЛьготы.НоваяКолонка("Льгота");
	тбВсеЛьготы.НоваяКолонка("Ставка","Число",7,4);   
	тбВсеЛьготы.НоваяКолонка("КвоЛьгот","Число",10,4);   
	
	СпрЛьготы = СоздатьОбъект("Справочник.ЛьготыСотрудника");
	СпрЛьготы.ИспользоватьВладельца(Сотрудник);
	СпрЛьготы.ВыбратьЭлементы();      
	
	спВрем = СоздатьОбъект("СписокЗначений");
	
	КвоМинимумов = 0;
	Пока СпрЛьготы.ПолучитьЭлемент() = 1 Цикл
		Если СпрЛьготы.ПометкаУдаления() = 1 Тогда
			Продолжить;
		КонецЕсли;
		Льгота = "" + СпрЛьготы.Льгота;
		Если (Найти(СпрЛьготы.Льгота.Код,"ЛьготыНДФЛ") = 0) Тогда
			//какие - то льготы для других налогов
			Продолжить;
		КонецЕсли;
		Если (СпрЛьготы.ДатаНачалаДействия &gt; КонПериода) Или ((ПустоеЗначение(СпрЛьготы.ДатаОкончанияДействия) = 0)
		И (СпрЛьготы.ДатаОкончанияДействия &lt; НачПериода)) Тогда
			// неактуальная льгота
			Продолжить;
		КонецЕсли;
		глПарсить(СпрЛьготы.Льгота.Дополнительно.Получить(КонПериода),спВрем);
		
		// добавим в таблицу найденную льготу   
		тбВсеЛьготы.НоваяСтрока();
		тбВсеЛьготы.Льгота = СпрЛьготы.Льгота.Код;
		тбВсеЛьготы.Ставка = СпрЛьготы.Льгота.Ставка.Получить(КонПериода);   
		тбВсеЛьготы.КвоЛьгот = 1;   
	КонецЦикла;
	тбВсеЛьготы.Свернуть("Льгота,Ставка","КвоЛьгот");  
	
	//Если льготы в справочнике продублировались заменим КвоЛгот на 1, 
	//а если льгота на детей посчитаем к-во детей:
	тбВсеЛьготы.ВыбратьСтроки();
	Пока тбВсеЛьготы.ПолучитьСтроку() = 1 Цикл   
		Если (Найти(тбВсеЛьготы.Льгота,"612а") &gt; 0) Тогда 
			тбВсеЛьготы.КвоЛьгот = глПолучитьКвоДетей(Сотрудник, КонПериода, 18); 
		ИначеЕсли (Найти(тбВсеЛьготы.Льгота,"612б") &gt; 0)	Тогда 
			тбВсеЛьготы.КвоЛьгот = глПолучитьКвоДетей(Сотрудник, КонПериода, 18, 1); 
		ИначеЕсли (Найти(тбВсеЛьготы.Льгота,"612в") &gt; 0) Тогда  
			// на троих детей
			КвоЛьгот = глПолучитьКвоДетей(Сотрудник, КонПериода, 18); 
			тбВсеЛьготы.КвоЛьгот = ?(КвоЛьгот &gt;= 3, КвоЛьгот, 0); 
		Иначе    
			тбВсеЛьготы.КвоЛьгот = 1;   
		КонецЕсли;
		тбВсеЛьготы.КвоЛьгот = тбВсеЛьготы.КвоЛьгот * тбВсеЛьготы.Ставка;
	КонецЦикла;
	тбВсеЛьготы.Сортировать("-КвоЛьгот");  
	
	// берем максимальную п.6.2 в случае, когда плательщик имеет право на несколько
	тбВсеЛьготы.ВыбратьСтроки();
	Если тбВсеЛьготы.ПолучитьСтроку() = 1 Тогда   
		глТрассировка("Расчет льгот для налога на доходы физ.лица. У " + Сотрудник + " участвует льгота " + тбВсеЛьготы.Льгота,3);
		глТрассировка("    К-во льгот = " + тбВсеЛьготы.КвоЛьгот,3);
		Если тбВсеЛьготы.Ставка = 1 Тогда  
			ВидЛьготы = тбВсеЛьготы.Льгота;//"01";
		ИначеЕсли тбВсеЛьготы.Ставка = 1.5 Тогда
			ВидЛьготы = тбВсеЛьготы.Льгота;//"02";
		ИначеЕсли тбВсеЛьготы.Ставка = 2 Тогда
			ВидЛьготы = тбВсеЛьготы.Льгота;//"03";
		КонецЕсли;	
		
	КонецЕсли;  
	
	ВидЛьготы = СтрЗаменить(ВидЛьготы,"Льготы","");
	ВидЛьготы = СтрЗаменить(ВидЛьготы,"є","э");
	ВидЛьготы = СтрЗаменить(ВидЛьготы,"баз","_611");

	Возврат СокрЛП(ВидЛьготы);</Текст>
