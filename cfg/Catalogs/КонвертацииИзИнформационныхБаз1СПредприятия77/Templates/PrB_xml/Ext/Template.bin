		<Алгоритм Имя="ПолучитьПолнуюСтавку" ИспользуетсяПриЗагрузке="false">
			<Текст>	ноВзнос		= ИсходящиеДанные;
	ДатаНач		= ВходящиеДанные;
	ДатаКон		= Источник;

	
	КалендарьВремени = Константа.ОсновнойГрафикРаботы.ОсновныеЧасы;
	ЕдиницаВремени = ?(Константа.ОсновнойГрафикРаботы.СуммированныйУчетВремени = 1, Перечисление.ЕдиницыВремени.Часы, ВДнях);
	НормаВремениЗаМесяц = ?(ЕдиницаВремени=ВДнях, КалендарьВремени.Дней(ДатаНач, ДатаКон), КалендарьВремени.Часов(ДатаНач, ДатаКон));
	
	НачСтавка = ноВзнос.Ставка.Получить(ДатаНач);
	ПолнаяСтавка = ноВзнос.Ставка.Получить(ДатаКон);
	Если (НачСтавка &lt;&gt; ПолнаяСтавка) И (НормаВремениЗаМесяц &lt;&gt; 0) Тогда
		// ставка налога менялась в течении месяца, поэтому рассчитаем сумму
		// пропорционально отработанным рабочим дням
		Ставка = НачСтавка;
		НормаВремени = 0;
		ПолнаяСтавка = 0;
		Для ТекДата = ДатаНач по ДатаКон Цикл
			ТекСтавка = ноВзнос.Ставка.Получить(ТекДата);
			Если Ставка &lt;&gt; ТекСтавка Тогда
				//Считаем коэфициент и обнуляем показатели для расчета нового
				ПолнаяСтавка = ПолнаяСтавка + Ставка * НормаВремени/НормаВремениЗаМесяц;
				Ставка = ТекСтавка;
				НормаВремени = 0;
			КонецЕсли;
			НормаВремени = НормаВремени + ?(ЕдиницаВремени=ВДнях,КалендарьВремени.Дней(ТекДата, ТекДата),КалендарьВремени.Часов(ТекДата, ТекДата));
		КонецЦикла;
		ПолнаяСтавка = ПолнаяСтавка + Ставка * НормаВремени/НормаВремениЗаМесяц;
	КонецЕсли;
	
	Возврат ПолнаяСтавка;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ПолучитьСтруктуруАдреса" ИспользуетсяПриЗагрузке="false">
			<Текст>	Адрес = ВходящиеДанные.Получить("Адрес");
	
	// глРазложить
	Стр = Адрес;
	Разделитель = ",";
	Список = СоздатьОбъект("СписокЗначений");
	Длина  = СтрДлина(Разделитель);
	Стр = СокрЛП(Стр);
	Поз = Найти(Стр, Разделитель);
	Пока 0 &lt; Поз Цикл
		Список.ДобавитьЗначение(СокрП(Лев(Стр, Поз-1)));
		Стр = СокрЛ(Сред(Стр, Поз+Длина));
		Поз = Найти(Стр, Разделитель);
	КонецЦикла;
    Список.ДобавитьЗначение(Стр);

	АдресСписком = Список;
	
	Если СтрКоличествоСтрок(Адрес) &lt;&gt; 1 Тогда     // недолжно быть переводов строк
		АдресСоответствуетТребованиямМНС = 0;
		
	ИначеЕсли СтрЧислоВхождений(Адрес, ",") &lt;&gt; 7 Тогда // должно быть 7 зпт
		АдресСоответствуетТребованиямМНС = 0;
	Иначе
		АдресСоответствуетТребованиямМНС = 1;
	КонецЕсли;
	
	Если АдресСоответствуетТребованиямМНС = 1 Тогда
		ВходящиеДанные.Установить("Поле1", АдресСписком.ПолучитьЗначение(1)); // индекс
		ВходящиеДанные.Установить("Поле2", АдресСписком.ПолучитьЗначение(2)); // регион
		ВходящиеДанные.Установить("Поле3", АдресСписком.ПолучитьЗначение(3)); // район
		ВходящиеДанные.Установить("Поле5", АдресСписком.ПолучитьЗначение(4)); // населенный пункт
		ВходящиеДанные.Установить("Поле6", АдресСписком.ПолучитьЗначение(5)); // улица
		ВходящиеДанные.Установить("Поле7", АдресСписком.ПолучитьЗначение(6)); // дом
		ВходящиеДанные.Установить("Поле8", АдресСписком.ПолучитьЗначение(7)); // корпус
		ВходящиеДанные.Установить("Поле9", АдресСписком.ПолучитьЗначение(8)); // квартира
		
		ТекПредставление = "";

		Если СокрЛП(ВходящиеДанные.Получить("Поле1")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", " + СокрЛП(ВходящиеДанные.Получить("Поле1"));
		КонецЕсли;

		Если СокрЛП(ВходящиеДанные.Получить("Поле2")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", " + СокрЛП(ВходящиеДанные.Получить("Поле2"));
		КонецЕсли;

		Если СокрЛП(ВходящиеДанные.Получить("Поле3")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", " + СокрЛП(ВходящиеДанные.Получить("Поле3"));
		КонецЕсли;
		
		Если СокрЛП(ВходящиеДанные.Получить("Поле5")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", " + СокрЛП(ВходящиеДанные.Получить("Поле5"));
		КонецЕсли;

		Если СокрЛП(ВходящиеДанные.Получить("Поле6")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", " + СокрЛП(ВходящиеДанные.Получить("Поле6"));
		КонецЕсли;

		Если СокрЛП(ВходящиеДанные.Получить("Поле7")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", дом № " + СокрЛП(ВходящиеДанные.Получить("Поле7"));
		КонецЕсли;

		Если СокрЛП(ВходящиеДанные.Получить("Поле8")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", корпус " + СокрЛП(ВходящиеДанные.Получить("Поле8"));
		КонецЕсли;

		Если СокрЛП(ВходящиеДанные.Получить("Поле9")) &lt;&gt; "" Тогда
			ТекПредставление = ТекПредставление + ", кв." + СокрЛП(ВходящиеДанные.Получить("Поле9"));
		КонецЕсли;

		Если СтрДлина(ТекПредставление) &gt; 2 Тогда
			ТекПредставление = Сред(ТекПредставление, 3);
		КонецЕсли;
		
		ВходящиеДанные.Установить("Представление", ТекПредставление);
		
	Иначе
		ВходящиеДанные.Установить("Представление", Адрес);
	КонецЕсли;</Текст>
		</Алгоритм>
