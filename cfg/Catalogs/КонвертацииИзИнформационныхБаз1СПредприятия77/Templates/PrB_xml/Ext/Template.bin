				ТабТЧ.НоваяКолонка("Сотрудник");
				ТабТЧ.НоваяКолонка("Организация");
				ТабТЧ.НоваяКолонка("ДоходПосле");
				ТабТЧ.НоваяКолонка("Период");
				
				ТабДок.НоваяСтрока();
				ТабДок.Дата = ДатаВводаОстатков;
				ТабДок.Организация = Организация;
				ТабДок.ПериодРегистрации = НачМесяца(ДатаВводаОстатков);
				
				СпрСотр = СоздатьОбъект("Справочник.Сотрудники");
				СпрСотр.ВыбратьЭлементы(); 
				Пока СпрСотр.ПолучитьЭлемент() = 1 Цикл 
					Если (СпрСотр.ДатаНачалаДействия &gt; ДатаВводаОстатков)
						или ((СпрСотр.ДатаОкончанияДействия &lt; ДатаВводаОстатков)
							 И (ПустоеЗначение(СпрСотр.ДатаОкончанияДействия)=0)) Тогда
						Продолжить;
					КонецЕсли;
					
					Если СпрСотр.Фирма &lt;&gt; Организация Тогда
						Продолжить;
					КонецЕсли;	
					
					Объект = СпрСотр.ТекущийЭлемент(); 
		
					// посмотрим историю
					РезДата = ДобавитьМесяц(глДатаНачалаРасчетаИндексации, -1);
					ДопДата = ДобавитьМесяц(глДатаНачалаРасчетаИндексации, -1);
					
					Пр = СоздатьОбъект("Периодический"); 
					Пр.ИспользоватьОбъект("ПризнакИзмененияДоходаДляИндексации",Объект);  
					Пр.ОбратныйПорядок();
					Пр.ВыбратьЗначения();
					Пока Пр.ПолучитьЗначение() = 1 Цикл
						Если (Пр.ДатаЗнач &lt;= ДатаВводаОстатков) и (Пр.Значение &lt;&gt; 0) Тогда
							РезДата = НачМесяца(Пр.ДатаЗнач); 
							Прервать;
						КонецЕсли;
					КонецЦикла;  
					
					// надо проверить еще и штатное расписание Тогда
					ОсновноеНазначение = Объект.ОсновноеНазначение.Получить(ДатаВводаОстатков); 
					МестоРаботы = ОсновноеНазначение.МестоРаботы; 
					Пр.ИспользоватьОбъект("ПризнакИзмененияДоходаДляИндексации",МестоРаботы);  
					Пр.ОбратныйПорядок();
					Пр.ВыбратьЗначения();
					Пока Пр.ПолучитьЗначение() = 1 Цикл
						Если (Пр.ДатаЗнач &lt;= ДатаВводаОстатков) и (Пр.Значение &lt;&gt; 0) Тогда
							ДопДата = НачМесяца(Пр.ДатаЗнач); 
							Прервать;
						КонецЕсли;
					КонецЦикла;  
					
					Если ОсновноеНазначение.ДокументНачала.Вид() = "ПриказОПриеме" Тогда
						ПервНазначение = ОсновноеНазначение;
					Иначе
						ПервНазначение = ОсновноеНазначение;
						Пока ПервНазначение.ДокументНачала.Вид() &lt;&gt; "ПриказОПриеме" Цикл
							Если ПервНазначение.ДокументНачала.Вид() = "ПриказОКадровомПеремещении" Тогда
								ПервНазначение = ПервНазначение.ДокументНачала.СтароеНазначение;
							Иначе
								Прервать;
							КонецЕсли;	
						КонецЦикла;	
					КонецЕсли;
					  
					//РезДата = Макс(ДобавитьМесяц(РезДата, 1), ДобавитьМесяц(ДопДата, 1), глДатаНачалаРасчетаИндексации);
					Если ((ДобавитьМесяц(РезДата, 1)&gt;ДобавитьМесяц(ДопДата, 1)) 
					И (ДобавитьМесяц(РезДата, 1)&gt;глДатаНачалаРасчетаИндексации)) Тогда
						ТабТЧ.НоваяСтрока();
						ТабТЧ.Организация = Организация;
						ТабТЧ.Сотрудник = ПервНазначение;
						ТабТЧ.Период = ДобавитьМесяц(РезДата, 1);    
						ТабТЧ.ДоходПосле = 1; 
					ИначеЕсли ((ДобавитьМесяц(ДопДата, 1)&gt;ДобавитьМесяц(РезДата, 1)) 
					И (ДобавитьМесяц(ДопДата, 1)&gt;глДатаНачалаРасчетаИндексации)) Тогда
						ТабТЧ.НоваяСтрока();
						ТабТЧ.Организация = Организация;
						ТабТЧ.Сотрудник = ПервНазначение;
						ТабТЧ.Период = ДобавитьМесяц(РезДата, 1);    
						ТабТЧ.ДоходПосле = 1; 
					КонецЕсли;
		
					
				КонецЦикла;	
				
				ТабТЧ.Сортировать("Период, Сотрудник");
				ТабДок.ПараметрыРасчетаИндексации = ТабТЧ;
		
				ВыборкаДанных = ТабДок;
КонецЕсли;</ПередОбработкойПравила>
