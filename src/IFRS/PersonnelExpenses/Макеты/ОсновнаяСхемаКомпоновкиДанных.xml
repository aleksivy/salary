			<field xsi:type="DataSetFieldField">
				<dataPath>СубконтоДт1</dataPath>
				<field>СубконтоДт1</field>
				<appearance/>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>СубконтоКт1</dataPath>
				<field>СубконтоКт1</field>
				<appearance/>
			</field>
