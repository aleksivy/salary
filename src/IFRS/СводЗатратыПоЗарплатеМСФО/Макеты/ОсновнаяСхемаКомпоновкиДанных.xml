////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Регистратор,
	Рег.СчетКт,
	Рег.СубконтоДт1 КАК Подразделение,
	Рег.СубконтоДт2 КАК СтатьяЗатрат,
	Рег.СубконтоДт3 КАК Номенклатура,
	ВЫБОР
		КОГДА Рег.СубконтоКт1 ЕСТЬ NULL
			ТОГДА НЕОПРЕДЕЛЕНО
		ИНАЧЕ Рег.СубконтоКт1
	КОНЕЦ КАК СубконтоКт1,
	ВЫБОР
		КОГДА Рег.СубконтоКт2 ЕСТЬ NULL
			ТОГДА НЕОПРЕДЕЛЕНО
		ИНАЧЕ Рег.СубконтоКт2
	КОНЕЦ КАК СубконтоКт2,
	ВЫБОР
		КОГДА Рег.СубконтоКт3 ЕСТЬ NULL
			ТОГДА НЕОПРЕДЕЛЕНО
		ИНАЧЕ Рег.СубконтоКт3
	КОНЕЦ КАК СубконтоКт3,
	Рег.СуммаОборот,
	Рег.ВалютнаяСуммаОборотДт
ПОМЕСТИТЬ _02_ВсеЗатраты
ИЗ
	РегистрБухгалтерии.МСФО.ОборотыДтКт(
			НАЧАЛОПЕРИОДА(&amp;ДатаНач, ДЕНЬ),
			КОНЕЦПЕРИОДА(&amp;ДатаКон, ДЕНЬ),
			Регистратор,
			СчетДт = &amp;Счет92,
			,
			,
			,
			СубконтоДт2 В
				(ВЫБРАТЬ
					НужныеСтатьи.СтатьяЗатрат
				ИЗ
					_01_НужныеСтатьи КАК НужныеСтатьи)) КАК Рег
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокТЧ.Ссылка,
	ДокТЧ.СубконтоДт1 КАК Подразделение,
	ВЫБОР
		КОГДА ДокТЧ.СубконтоДт1.Родитель = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА ДокТЧ.СубконтоДт1
		КОГДА ДокТЧ.СубконтоДт1.Родитель.Родитель = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА ДокТЧ.СубконтоДт1.Родитель
		КОГДА ДокТЧ.СубконтоДт1.Родитель.Родитель.Родитель = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА ДокТЧ.СубконтоДт1.Родитель.Родитель
	КОНЕЦ КАК Подразделение2,
	ДокТЧ.СубконтоДт2 КАК СтатьяЗатрат,
	ДокТЧ.СубконтоДт3 КАК Номенклатура,
	ДокТЧ.СчетКт,
	ДокТЧ.СубконтоКт1,
	ДокТЧ.СубконтоКт2,
	ДокТЧ.СубконтоКт3,
	СУММА(ДокТЧ.Сумма) КАК Сумма
ПОМЕСТИТЬ _03_Начисления
ИЗ
	Документ.НачислениеЗарплатыРаботникамОрганизаций.ОтражениеВУчете КАК ДокТЧ
ГДЕ
	ДокТЧ.Ссылка В
			(ВЫБРАТЬ
				ВсеЗатраты.Регистратор
			ИЗ
				_02_ВсеЗатраты КАК ВсеЗатраты)
	И ДокТЧ.СчетДт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.АдминистративныеРасходы)

СГРУППИРОВАТЬ ПО
	ДокТЧ.СубконтоКт3,
	ДокТЧ.СчетКт,
	ДокТЧ.Ссылка,
	ДокТЧ.СубконтоДт1,
	ДокТЧ.СубконтоКт1,
	ДокТЧ.СубконтоДт2,
	ДокТЧ.СубконтоДт3,
	ДокТЧ.СубконтоКт2,
	ВЫБОР
		КОГДА ДокТЧ.СубконтоДт1.Родитель = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА ДокТЧ.СубконтоДт1
		КОГДА ДокТЧ.СубконтоДт1.Родитель.Родитель = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА ДокТЧ.СубконтоДт1.Родитель
		КОГДА ДокТЧ.СубконтоДт1.Родитель.Родитель.Родитель = ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)
			ТОГДА ДокТЧ.СубконтоДт1.Родитель.Родитель
	КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Начисления.Ссылка,
	Начисления.Подразделение2,
	Начисления.СтатьяЗатрат,
	Начисления.Номенклатура,
	Начисления.СчетКт,
	Начисления.СубконтоКт1,
	Начисления.СубконтоКт2,
	Начисления.СубконтоКт3,
	СУММА(Начисления.Сумма) КАК Сумма
ПОМЕСТИТЬ _04_НачисленияСвернуто
ИЗ
	_03_Начисления КАК Начисления

СГРУППИРОВАТЬ ПО
	Начисления.СубконтоКт2,
	Начисления.СчетКт,
	Начисления.Подразделение2,
	Начисления.Номенклатура,
	Начисления.СубконтоКт1,
	Начисления.СубконтоКт3,
	Начисления.Ссылка,
	Начисления.СтатьяЗатрат
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Начисления.Ссылка,
	Начисления.Подразделение,
	Начисления.Подразделение2,
	Начисления.СтатьяЗатрат,
	Начисления.Номенклатура,
	Начисления.СчетКт,
	Начисления.СубконтоКт1,
	Начисления.СубконтоКт2,
	Начисления.СубконтоКт3,
	Начисления.Сумма,
	НачисленияСвернуто.Сумма КАК СуммаСвернуто
ПОМЕСТИТЬ _05_НачисленияВсе
ИЗ
	_03_Начисления КАК Начисления
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ _04_НачисленияСвернуто КАК НачисленияСвернуто
		ПО Начисления.Ссылка = НачисленияСвернуто.Ссылка
			И Начисления.Подразделение2 = НачисленияСвернуто.Подразделение2
			И Начисления.СтатьяЗатрат = НачисленияСвернуто.СтатьяЗатрат
			И Начисления.Номенклатура = НачисленияСвернуто.Номенклатура
			И Начисления.СчетКт = НачисленияСвернуто.СчетКт
			И Начисления.СубконтоКт1 = НачисленияСвернуто.СубконтоКт1
			И Начисления.СубконтоКт2 = НачисленияСвернуто.СубконтоКт2
			И Начисления.СубконтоКт3 = НачисленияСвернуто.СубконтоКт3
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВсеЗатраты.Регистратор,
	ЕСТЬNULL(НачисленияВсе.Подразделение, ВсеЗатраты.Подразделение) КАК Подразделение,
	ЕСТЬNULL(НачисленияВсе.Подразделение.Name, ВсеЗатраты.Подразделение.Name) КАК ПодразделениеName,
	НужныеСтатьи.ТипЗатраты,
	ВЫБОР
		КОГДА НужныеСтатьи.ТипЗатраты = "4.OTHER PERSONNEL EXPENSES"
			ТОГДА ВсеЗатраты.СтатьяЗатрат
		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.СтатьиЗатрат.ПустаяСсылка)
	КОНЕЦ КАК ТипЗатраты2,
	ВсеЗатраты.СтатьяЗатрат,
	ВсеЗатраты.СчетКт,
	ВсеЗатраты.СубконтоКт1,
	ВсеЗатраты.СубконтоКт2,
	ВсеЗатраты.СубконтоКт3,
	ВЫБОР
		КОГДА ВсеЗатраты.ВалютнаяСуммаОборотДт = ЕСТЬNULL(НачисленияВсе.СуммаСвернуто, 0)
			ТОГДА ВсеЗатраты.СуммаОборот * ЕСТЬNULL(НачисленияВсе.Сумма, 0) / ВсеЗатраты.ВалютнаяСуммаОборотДт
		ИНАЧЕ ВсеЗатраты.СуммаОборот
	КОНЕЦ КАК СуммаДол,
	ВЫБОР
		КОГДА ВсеЗатраты.ВалютнаяСуммаОборотДт = ЕСТЬNULL(НачисленияВсе.СуммаСвернуто, 0)
