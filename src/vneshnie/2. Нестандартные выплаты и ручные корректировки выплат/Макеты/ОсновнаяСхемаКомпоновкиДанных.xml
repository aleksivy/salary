			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Рег.Авторасчет КАК Авторасчет,
	Рег.Регистратор КАК Регистратор,
	Рег.ПериодРегистрации КАК Дата,
	Рег.Регистратор.Номер КАК Номер,
	Рег.НомерСтроки,
	Рег.Сотрудник,
	Рег.ВидРасчета КАК ВидНачисления,
	Рег.Результат КАК СуммаНачислено,
	Рег.Показатель1 КАК СуммаБазовая,
	Рег.ОплаченоДнейЧасов КАК ОплаченоДнейЧасов,
	Рег.НормаДней,
	Рег.НормаДнейЗаМесяц,
	Рег.КалендарныеДни,
	Рег.ПодразделениеОрганизации,
	Рег.Сотрудник.ТекущаяДолжностьОрганизации КАК Должность
ИЗ
	РегистрРасчета.ОсновныеНачисленияРаботниковОрганизаций КАК Рег
ГДЕ
	Рег.ПериодРегистрации = НАЧАЛОПЕРИОДА(&amp;ДатаОтчета, МЕСЯЦ)
	И Рег.Результат &gt; 0
	И (Рег.Сотрудник.ДатаУвольнения = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ Рег.Сотрудник.ДатаУвольнения &gt; НАЧАЛОПЕРИОДА(&amp;ДатаОтчета, МЕСЯЦ))
	И (НЕ Рег.Авторасчет
			ИЛИ НЕ Рег.ВидРасчета = ЗНАЧЕНИЕ(ПланВидовРасчета.ОсновныеНачисленияОрганизаций.ОкладПоДням)
				И НЕ Рег.ВидРасчета = ЗНАЧЕНИЕ(ПланВидовРасчета.ОсновныеНачисленияОрганизаций.ИндексацияЗарплаты)
				И НЕ Рег.ВидРасчета = ЗНАЧЕНИЕ(ПланВидовРасчета.ОсновныеНачисленияОрганизаций.ОплатаПоСреднемуОтп)
				И НЕ Рег.ВидРасчета = ЗНАЧЕНИЕ(ПланВидовРасчета.ОсновныеНачисленияОрганизаций.ДополнительныйЕжегодныйОтпуск)
				И НЕ Рег.ВидРасчета = ЗНАЧЕНИЕ(ПланВидовРасчета.ОсновныеНачисленияОрганизаций.ОплатаПоСреднему))</query>
		</dataSet>
