
Процедура КнопкаВыполнитьНажатие(Кнопка)
	Если Не ЗначениеЗаполнено(Ссылка) Тогда
		Сообщить("Не выбран документ для заполнения!");
	ИначеЕсли Не Ссылка.Проведен Тогда
		Сообщить("Выбранный документ не проведен!");
	Иначе
		мОбъект = Ссылка.ПолучитьОбъект();
		мИмяТабличнойЧасти = "ОсновныеНачисления";
		
		Заполнить();
		мОбъект.Записать(РежимЗаписиДокумента.Проведение);
		
	КонецЕсли;	
